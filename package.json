{
  "name": "canvas-ts-project",
  "version": "0.1.0",
  "homepage": "https://mmkln.github.io/majom-canvas",
  "description": "Проект Canvas з кружечками, стрілками та видаленням на TypeScript",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "start": "vite",
    "start:8080": "vite --port=8080",
    "start:4200": "vite --port=4200",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "keywords": [
    "canvas",
    "typescript",
    "drawing",
    "deletion"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "gh-pages": "^5.0.0",
    "prettier": "^3.5.1",
    "typescript": "^5.0.0",
    "vite": "^6.2.3"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "jwt-decode": "^4.0.0",
    "rxjs": "^7.8.2",
    "rxjs-http-client": "^8.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0"
  }
}
